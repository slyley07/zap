<!-- <div class="login"> -->
<div class="zapHead">
	<h1>
		Zap In!
	</h1>
</div>

<div class="inFull">
		<%= form_tag @user do %>
			<div class="logForm">
					<%= text_field_tag :email, nil, placeholder: 'Alias' %>

					<br>

					<%= password_field_tag :password, nil, placeholder: 'Password' %>

					<br>
			</div>

			<%=	submit_tag "ZAP!" %>
		<% end %>
</div>
